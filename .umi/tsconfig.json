{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "moduleResolution": "bundler",
    "importHelpers": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "sourceMap": true,
    "baseUrl": "../",
    "strict": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "paths": {
      "@/*": [
        "*"
      ],
      "@@/*": [
        ".umi/*"
      ],
      "umi": [
        "/Users/wvvw/fun/zwds/zw/node_modules/umi"
      ],
      "umi/typings": [
        ".umi/typings"
      ]
    }
  },
  "include": [
    "../.umirc.ts",
    "../**/*.d.ts",
    "../**/*.ts",
    "../**/*.tsx"
  ]
}
